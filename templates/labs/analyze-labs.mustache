    {{{includes.navbar}}}
    <div class="monarch-container">
        <div class="content">
            <div class="wrapperforfooter">
            <div class="jumbotron">
                <div id="description">
                    {{#alerts}}
                    <div class="alert">
                        {{{.}}}
                    </div>
                    {{/alerts}}
                    <img id="monarch-logo" class="downloadline" src="/image/logo.png"/>
                    <h1 class="downloadline">Monarch Phenotype Profile Analysis</h1>
                    <a class="downloadline downloadline-left btn btn-large btn-default btn-monarch-main monarch-feedback-dark"
                            href="mailto:info@monarchinitiative.org">Your feedback welcome!</a>
                    {{#isPhenotype}}
                    <p>Find models and phenotypes similar to a set of abnormal phenotypes of interest.</p>
                    {{/isPhenotype}}
                </div>
                <div id="categories">
                    {{^hasInputItems}}
                    <a href="#query"><div id="initialtab" class="tab contenttab second">Search</div></a>
                    <div class="tab greyed-out">Results</div>
                    {{/hasInputItems}}
                    {{#hasInputItems}}
                    <a href="#query"><div class="tab contenttab second">Search</div></a>
                    <a href="#result"><div id="initialtab" class="tab contenttab">Results</div></a>
                    {{/hasInputItems}}
                </div>
            </div>
            <div id="complete-info">
                {{^hasInputItems}}
                <div id="query" class="first category">
                {{/hasInputItems}}
                {{#hasInputItems}}
                <div id="query" class="category">
                {{/hasInputItems}}
                <div class="row-fluid">
                 <form role="form" action="/labs/analyze/{{datatype}}/" method="GET">
                  <div class="col-xs-6 col-sm-6 col-md-6 col-lg-6">
                        <input id="analyze_auto_target" name="input_items" value="{{input_items}}" type="hidden"/>
                        <div id="analyze-searchlist" class="form-group">
                          <label for="analyze_auto_input"><h4>Add to search list</h4>
                            {{#isPhenotype}}
                            <p class="example">Type phenotype name for autocomplete (e.g.
                            Renal agenesis, Multicystic kidney dysplasia, etc.)</p>
                            {{/isPhenotype}}
                          </label>
                          <input id="analyze_auto_input" type="text" class="form-control"
                          placeholder="Enter {{datatype}} here"/>
                        </div>
                        <div id="analyze-profile" class="form-group">
                            <label for="analyze_auto_list"><h4>Search list / {{datatype}} profile</h4>
                            {{#isPhenotype}}
                            <p class="example">Search for profiles similar to this list of phenotypes.</p>
                            {{/isPhenotype}}
                            </label>
                            <ul id="analyze_auto_list" class="list-group">
                            <li id="analyze-empty-item" class="list-group-item">Empty: Add items using the input above.</li>
                            </ul>
                        </div>
                        <button type="submit" class="btn btn-info">Search</button>  
                  </div>
                  <div class="col-xs-6 col-sm-6 col-md-6 col-lg-6">
                    <div class="panel panel-default">
                      <div class="panel-heading">
                        <h4 class="panel-title text-center">Settings</h4>
                      </div>
                      <div class="row">
                        <div id="analyze-search" class="col-xs-6 col-sm-6 col-md-6 col-lg-6">
                          <div class="pheno-radio-button">
                            <label id="pheno-search">
                              <input id="srch" type="radio" checked="true" value="search" name="mode"> Search</label>
                          </div>
                          <div class="explain">
                            Search for model genes, genotypes, and semantically similar diseases
                          </div>
                          <div id="search-form-group">
                            <div class="row">
                              <div class="form-group" id="analyze-target">
                                <label for="target" class="col-6 control-label search-label">Target Species:</label>
                                <div class="col-6">
                                  <select class="form-control" id="target" name="target_species">
                                    <option value="all" selected="selected" >All</option>
                                    <option value="10090">Mouse (genes)</option>
                                    <option value="7955">Zebrafish (genes)</option>
                                    <option value="9606">Human (diseases)</option>
                                  </select>
                                </div>
                              </div>
                            </div>
                            <div class="form-group" id="analyze-type">
                              <div class="row">
                                <label for="type" class="col-6 control-label search-label">Target Type:</label>
                                <div class="col-6">
                                  <select class="form-control" id="type" name="target_type" >
                                    <option value="all" selected="selected" >All</option>
                                    <option value="disease" disabled>Disease</option>
                                    <option value="gene" disabled>Gene</option>
                                    <option value="genotype" disabled>Genotype</option>
                                  </select>
                                </div>
                              </div>
                            </div>
                            <div id="analyze-limit" class="form-group">
                              <div class="row">
                                <label for="analyze_limit_input" class="col-6 control-label search-label">Maximum number of results:</label>
                                <div class="col-6">
                                  <input class="form-control" id="analyze_limit_input" name="limit" type="text"
                                      value="{{limit}}"></input>
                                </div>
                              </div> 
                            </div>
                          </div>
                      </div>
                    
                    
                      <div id= "analyze-compare" class="col-xs-6 col-sm-6 col-md-6 col-lg-6">
                        <div class="pheno-radio-button">
                          <label id="pheno-compare">
                            <input id="compare" type="radio" value="compare" name="mode"> Compare</label>
                        </div>
                        <div class="explain">
                            Compare phenotypes against a set of genes<br><br>
                        </div>
                        <div id="compare-form-group">
                          <div id="gene-text-area">Gene IDs:<br>
                            <span class="small">
                                (Gene IDs must correspond to Entrez 
                                gene IDs in the format NCBIGene:12345
                                and can be newline or comma separated)
                            </span>
                          </div>
                       
                          <textarea id="gene-list" class="form-control" rows="4" placeholder="" name="gene-list" disabled="true"></textarea>
                          <div id="gene-shortcuts">Add to list:<br>
                              <span class="small">
                                Given the above list of genes, add genes
                                based on relationship:
                              </span>
                          </div>
                          <button id="ortholog" type="button" class="btn btn-primary" disabled>Orthologs</button>
                          <button id="paralog" type="button" class="btn btn-primary" disabled>Paralogs</button>
                        </div>
                        <div id="ajax-spinner">
                          <div class="spinner" >
                            <div class="spinner-container container1">
                              <div class="circle1"></div>
                              <div class="circle2"></div>
                              <div class="circle3"></div>
                              <div class="circle4"></div>
                            </div>
                            <div class="spinner-container container2">
                              <div class="circle1"></div>
                              <div class="circle2"></div>
                              <div class="circle3"></div>
                              <div class="circle4"></div>
                            </div>
                            <div class="spinner-container container3">
                              <div class="circle1"></div>
                              <div class="circle2"></div>
                              <div class="circle3"></div>
                              <div class="circle4"></div>
                            </div>
                          </div>
                          <div id='fetching'>Fetching Genes...</div>
                        </div>
                        <div id='error-msg'>Error Fetching Genes</div>
                      </div>
                     </div>
                   </div>
                  </div>
                
                </form>
                </div>
               
                {{#hasInputItems}}
                </div>
                <div id="result" class="first category">
                    <div id ="phen_vis"></div>
                    <div id="result-table">
                      {{#hasTable}}
                        <h3 class="downloadline">Results</h3>
                        <a href="{{{downloadURL}}}" class="btn btn-default downloadline downloadline-left monarch-blue">JSON</a>
                        {{#hasResults}}
                          {{#singleSpecies}}
                            <p>Species: {{{speciesHref}}}</p>
                          {{/singleSpecies}}
                          <input id="analyze_auto_species" name="current_target_species" value="{{target_filter.species}}" type="hidden"/>
                          {{{resultsTable}}}
                          </table>
                        {{/hasResults}}
                        {{^hasResults}}
                        <div class="text">Unfortunately, your search produced no results.</div>
                        {{/hasResults}}
                      {{/hasTable}}
                    </div>
                </div>
                <div id="splitLabels" style="display:none">{{splitLabels}}</div>
                {{/hasInputItems}}
            </div>
            </div> <!-- closes wrapperforfooter -->
            {{{includes.footer}}}
        </div>
    </div>