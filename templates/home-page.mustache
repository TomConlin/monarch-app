<!-- Top-level internal container. -->
<!-- <div class="container-fluid" class="conflict-top-container-margin"> -->
<div id="monarch-home-container" class="container-fluid" style="margin: 0.8em;margin-top:0.3em;padding:0;">

  <div class="jumbotron monarch-home-splash">
    <span class="monarch-home-splash-text">
      <strong>The Monarch Initiative</strong>
    </span>
  </div>


  <!-- Open first row section. -->
  <div class="row">

    <div id="col-1" class="col-xs-12 col-sm-8 col-md-8 col-lg-8">
      <!-- Quiet rollover. -->
      <!-- WARNING: There is an HTML/CSS trick in here--the largest -->
      <!-- item is in "relative" position and the rest are in "absolute". -->
      <div class="panel-body" style="margin-top:-20px;">
        <div id="monarch-tabber" class="monarch-tabber">
          <div class="row monarch-tabber-controls">
            <div class="monarch-tabber-control">
              <a href="/page/about" class="btn btn-block btn-sm monarch-tabber-on-item monarch-tabber-control-btn">
                <span class="monarch-nav-item-title">Overview</span>
                <br/>
                <img src="/image/carousel-monarch.png"
                     class="monarch-nav-embedded-image" alt="Overview" />
              </a>
            </div>

            <div class="monarch-tabber-control">
              <a href="/disease" class="btn btn-block btn-sm monarch-tabber-off-item monarch-tabber-control-btn">
                <span class="monarch-nav-item-title">Diseases</span>
                <br/>
                <img src="/image/carousel-diseases.png"
                     class="monarch-nav-embedded-image" alt="Diseases" />
              </a>
            </div>

            <div class="monarch-tabber-control">
              <a href="/phenotype" class="btn btn-block btn-sm monarch-tabber-off-item monarch-tabber-control-btn">
                <span class="monarch-nav-item-title">Phenotypes&nbsp;</span>
                <br/>
                <img src="/image/carousel-phenotypes.png"
                     class="monarch-nav-embedded-image" alt="Phenotypes" />
              </a>
            </div>

            <div class="monarch-tabber-control">
              <a href="/model" class="btn btn-block btn-sm monarch-tabber-off-item monarch-tabber-control-btn">
                <span class="monarch-nav-item-title">Models</span>
                <br/>
                <img src="/image/carousel-models.png" class="monarch-nav-embedded-image" alt="Models" />
              </a>
            </div>

            <div class="monarch-tabber-control">
              <a href="/gene" class="btn btn-block btn-sm monarch-tabber-off-item monarch-tabber-control-btn">
                <span class="monarch-nav-item-title">Genes</span>
                <br/>
                <img src="/image/carousel-genes.png"
                     class="monarch-nav-embedded-image" alt="Genetics" />
              </a>
            </div>
          </div> <!-- monarch-tabber-controls -->
        </div> <!-- monarch-tabber -->
      </div>

      <hr class="monarch-home-carousel-separator-hr"/>

      <div class="panel-body">
        <div id="monarch-carousel" class="monarch-carousel">
          <div class="monarch-carousel-item monarch-carousel-item-selected monarch-carousel-item-largest">
            <!--<div>-->
              <!--<a role="button" href="/page/about">-->
                <!--<div class="btn btn-block btn-default monarch-carousel-explore-title">-->
                    <!--Traverse phenotypes, diseases, models, and genes with Monarch.-->
                <!--</div>-->
              <!--</a>-->

              <!--<hr class="monarch-home-carousel-item-hr"/>-->
            <!--</div>-->

            <div class="center-block text-center" style="clear:both;">
              <img class="monarch-carousel-item-image" src="/image/carousel-overview.png"
                   class="img-responsive"
                   alt="Explore Monarch" />
            </div>
          </div>

          <div class="monarch-carousel-item monarch-carousel-item-unselected">
            <!--<div>-->
              <!--&lt;!&ndash;<a role="button" href="/disease">&ndash;&gt;-->
                <!--<div class="btn btn-block btn-default monarch-carousel-explore-title">-->
                    <!--Diseases: Investigate genetic underpinnings of disease.-->
                <!--</div>-->
              <!--&lt;!&ndash;</a>&ndash;&gt;-->

              <!--<hr class="monarch-home-carousel-item-hr"/>-->
            <!--</div>-->

            <div class="center-block text-center" style="clear:both;">
              <img class="monarch-carousel-item-image" src="/image/explore-graph-dis.png"
                   class="img-responsive"
                   alt="Explore Diseases" />
            </div>
          </div>

          <div class="monarch-carousel-item monarch-carousel-item-unselected">
            <!--<div>-->
              <!--<a role="button" href="/phenotype">-->
                <!--<div class="btn btn-block btn-default monarch-carousel-explore-title">-->
                    <!--Phenotypes: Explore phenotypes that may be similar across various diseases or across species.-->
                <!--</div>-->
              <!--</a>-->

              <!--<hr class="monarch-home-carousel-item-hr"/>-->
            <!--</div>-->

            <div class="center-block text-center" style="clear:both;">
              <img class="monarch-carousel-item-image" src="/image/explore-graph-phe.png"
                   class="img-responsive"
                   alt="Explore Phenotypes" />
            </div>
          </div>

          <div class="monarch-carousel-item monarch-carousel-item-unselected">
            <!--<div>-->
              <!--<a role="button" href="/model">-->
                <!--<div class="btn btn-block btn-default monarch-carousel-explore-title">-->
                    <!--Models: Find model organisms for phenotypes & diseases.-->
                <!--</div>-->
              <!--</a>-->

              <!--<hr class="monarch-home-carousel-item-hr"/>-->

            <!--</div>-->

            <div class="center-block text-center" style="clear:both;">
              <img class="monarch-carousel-item-image" src="/image/explore-graph-mod.png"
                   class="img-responsive"
                   alt="Explore Models" />
            </div>
          </div>

          <div class="monarch-carousel-item monarch-carousel-item-unselected">
            <!--<div>-->
              <!--<a role="button" href="/gene">-->
                <!--<div class="btn btn-block btn-default monarch-carousel-explore-title">-->
                    <!--Genes: Explore the diseases and phenotypes for your favorite gene.-->
                <!--</div>-->
              <!--</a>-->

              <!--<hr class="monarch-home-carousel-item-hr"/>-->

            <!--</div>-->

            <div class="center-block text-center" style="clear:both;">
              <img class="monarch-carousel-item-image" src="/image/explore-graph-gen.png"
                   class="img-responsive"
                   alt="Explore Genes" />
            </div>
          </div>
        <a id="monarch-tabber-next" class="monarch-tabber-control-next">
          <div class="monarch-tabber-chevron monarch-tabber-right-chevron">
          </div>
        </a>
        <a id="monarch-tabber-prev" class="monarch-tabber-control-prev">
          <div class="monarch-tabber-chevron monarch-tabber-left-chevron">
          </div>
        </a>
        </div> <!-- monarch-carousel -->

      </div> <!-- panel-body -->

    </div> <!-- column 1 -->

    <!-- Two-column layout, col. 2 -->
    <div id="col-1" class="col-xs-12 col-sm-4 col-md-4 col-lg-4">

      <!-- Search. -->
      <div class="panel panel-default"> <!-- style="padding-bottom: 0;"> -->
        <div class="panel-heading">
          <h3 class="panel-title">Compare Phenotypes</h3>
        </div>
        <div class="panel-body">
          <span class="">
            Find models and diseases similar to a set of abnormal phenotypes of interest and then visualize their overlap.
          </span>
          <div class="center-block text-center" style="clear:both;">
            <a href="/analyze/phenotypes">
              <img src="/image/Phenogrid3Compare.png"
                   class="img-responsive monarch-home-phenotype-image"
                   alt="Explore Diseases" />
            </a>
          </div>
          <div class="center-block text-center">
            <a class="btn btn-primary center-block monarch-home-phenotype-analysis" href="/analyze/phenotypes">
              Go to Phenotype Analysis
            </a>
          </div>
        </div> <!-- panel-body -->
      </div> <!-- panel-default -->

      <!-- News and twitter combo. -->
      <div class="row">
        <div id="blog" class="col-xs-12 col-sm-12 col-md-12 col-lg-12">

          <div
            class="panel panel-default news-panel">
            <div class="panel-heading">
              <h3 class="panel-title">News and Events</h3>
            </div>
            <div class="panel-body news-list">
              {{#blog_results}}
                <div class="news-item">
                  <a href="{{link}}">
                     <span class="news-item-date">{{date}} &nbsp;</span>
                     <span class="news-item-title">{{title}}</span>
                  </a>
                  {{#category}}
                    <a href="{{link}}" class="label label-default">
                      {{label}}
                    </a>
                  {{/category}}
                </div>
              {{/blog_results}}
              <div class="news-more">
                <a href="http://monarch-initiative.blogspot.com/" alt="More posts">
                  More...
                </a>
              </div>
            </div>
          </div> <!-- news-panel -->

        </div> <!-- blog -->
      </div> <!-- row -->



      <div class="row">
        <div id="twitter" class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
          <a  class="twitter-timeline"
            href="https://twitter.com/search?q=%40MonarchInit+OR+%23raredisease+OR+%40GA4GH+OR+%40phenomecentral"
            data-widget-id="527567015918051328"
            data-chrome=""
            height="340"
            width="520">
            Loading Tweets about "@MonarchInit OR #raredisease OR @GA4GH OR @phenomecentral"
          </a>

          <script>
          // From https://dev.twitter.com/web/javascript/loading - 7/26/2015

          window.twttr = (function(d, s, id) {
            var js, fjs = d.getElementsByTagName(s)[0],
              t = window.twttr || {};
            if (d.getElementById(id)) return t;
            js = d.createElement(s);
            js.id = id;
            js.src = "https://platform.twitter.com/widgets.js";
            fjs.parentNode.insertBefore(js, fjs);

            t._e = [];
            t.ready = function(f) {
              t._e.push(f);
            };

            return t;
          }(document, "script", "twitter-wjs"));
          </script>
        </div>
      </div> <!-- row -->
    </div> <!-- col-lg-5 -->
  </div> <!-- row -->

</div> <!-- container-fluid -->
<!-- Close top-level container. -->
